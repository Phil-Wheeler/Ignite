{
  "ApplicationInsights": {
    "InstrumentationKey": "fea554cd-1213-4e1a-940a-aa7ca5b35e69"
  },
  "Data": {
    "DefaultConnection": {
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "NWebSec": {
    "securityHttpHeaders": {
      "ContentSecurityPolicy": true,
      "csp-sources": {
        "default-src": {
          "self": true,
          "sources": "fonts.gstatic.com"
        },
        "script-src": {
          "unsafeInline": true,
          "unsafeEval": true,
          "self": true,
          "sources": "https://wheeler.kiwi.nz,https://wheeler.azurewebsites.net,http://localhost:5000,https://cdnjs.cloudflare.com"
        },
        "style-src": {
          "unsafeInline": true,
          "self": true,
          "sources": "https://fonts.gstatic.com"
        },
        "img-src": {
          "sources": "https://www.google.com,https://www.google-analytics.com"
        },
        "font-src": {
          "sources": "https://cdnjs.cloudflare.com"
        },
        "object-src": {
          "none": false
        },
        "media-src": {
          "none": false
        },
        "frame-src": {
          "none": false
        },
        "connect-src": {
          "none": false
        },
        "frame-ancestors": {
          "none": false
        },
        "report-uri": {
          "enableBuiltinHandler": true
        }
      },
      "x-frame-options": {
        "policy": "SameOrigin"
      },
      "x-xss-protection": {
        "policy": {
          "filterEnabled": true,
          "blockMode": true
        }
      },
      "x-content-type-options": {
        "enabled": true
      },
      "strict-transport-security": {
        "max-age": 365,
        "includeSubdomains": true,
        "preload": true,
        "httpsOnly": false
      }
    }
  }
}